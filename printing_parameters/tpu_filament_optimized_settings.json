{
  "material": "TPU (Thermoplastic Polyurethane)",
  "version": "1.0",
  "optimization_date": "2024-12-19",
  "printer_compatibility": ["Ender 3", "Prusa i3", "Bambu Lab X1C"],
  "temperature_settings": {
    "nozzle_temperature": {
      "min": 210,
      "max": 230,
      "recommended": 220,
      "unit": "°C",
      "notes": "优化后的温度范围，确保良好的层间粘合"
    },
    "bed_temperature": {
      "min": 50,
      "max": 70,
      "recommended": 60,
      "unit": "°C",
      "notes": "适中的热床温度，防止翘曲同时便于取件"
    }
  },
  "speed_settings": {
    "print_speed": {
      "outer_perimeter": 15,
      "inner_perimeter": 20,
      "infill": 25,
      "first_layer": 10,
      "unit": "mm/s",
      "notes": "较慢的打印速度确保TPU材料有足够时间流动"
    },
    "travel_speed": {
      "value": 80,
      "unit": "mm/s",
      "notes": "适中的移动速度，减少拉丝"
    }
  },
  "retraction_settings": {
    "retraction_distance": {
      "direct_drive": 1.0,
      "bowden": 3.0,
      "unit": "mm",
      "notes": "根据挤出机类型优化的回缩距离"
    },
    "retraction_speed": {
      "value": 25,
      "unit": "mm/s",
      "notes": "较慢的回缩速度，避免拉断柔性材料"
    },
    "z_hop": {
      "enabled": true,
      "height": 0.2,
      "unit": "mm",
      "notes": "启用Z轴抬升，减少喷嘴拖拽"
    }
  },
  "layer_settings": {
    "layer_height": {
      "min": 0.15,
      "max": 0.3,
      "recommended": 0.2,
      "unit": "mm",
      "notes": "适中的层高，平衡打印质量和速度"
    },
    "first_layer_height": {
      "value": 0.25,
      "unit": "mm",
      "notes": "稍厚的首层，提高附着力"
    }
  },
  "infill_settings": {
    "infill_percentage": {
      "flexible_parts": 15,
      "structural_parts": 25,
      "unit": "%",
      "notes": "根据用途调整填充密度"
    },
    "infill_pattern": {
      "recommended": "gyroid",
      "alternatives": ["cubic", "triangular"],
      "notes": "陀螺仪填充模式提供良好的柔性支撑"
    }
  },
  "support_settings": {
    "support_material": "same_material",
    "support_overhang_angle": 60,
    "support_interface": {
      "enabled": true,
      "layers": 2,
      "notes": "支撑接触面设置，便于移除"
    }
  },
  "cooling_settings": {
    "part_cooling_fan": {
      "first_layer": 0,
      "subsequent_layers": 30,
      "unit": "%",
      "notes": "适度的冷却，避免TPU过度冷却变脆"
    },
    "minimum_layer_time": {
      "value": 15,
      "unit": "seconds",
      "notes": "确保每层有足够的冷却时间"
    }
  },
  "advanced_settings": {
    "linear_advance": {
      "enabled": true,
      "k_factor": 0.3,
      "notes": "线性提前量补偿，改善打印质量"
    },
    "pressure_advance": {
      "enabled": true,
      "value": 0.05,
      "notes": "压力提前量设置（适用于Klipper固件）"
    }
  },
  "troubleshooting": {
    "stringing": "降低温度5-10°C，增加回缩距离",
    "poor_adhesion": "提高热床温度，使用胶棒或PEI板",
    "layer_separation": "提高喷嘴温度，降低打印速度",
    "warping": "使用封闭式打印机或降低冷却风扇速度"
  },
